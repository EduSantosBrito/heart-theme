{
	"name": "heart-theme",
	"type": "dark",
	"colors": {
		"editor.background": "#212121",
		"editor.foreground": "#ffffff",
		"activityBar.background": "#212121",
		"activityBar.foreground": "#b3b3b3",
		"activityBar.dropBackground": "#121212",
		"activityBar.border": "#121212",
		"activityBarBadge.background": "#9B59B6",
		"activityBarBadge.foreground": "#fff",
		"sideBar.background": "#121212",
		"sideBar.foreground": "#b3b3b3",
		"sideBar.border": "#121212",
		"sideBarTitle.foreground": "#FFF",
		"sideBarSectionHeader.foreground": "#FFF",
		"sideBarSectionHeader.background": "#212121",
		"list.activeSelectionBackground": "#535353",
		"list.activeSelectionForeground": "#fff",
		"list.dropBackground": "#212121",
		"list.hoverBackground": "#212121",
		"list.hoverForeground": "#fff",
		"list.focusBackground": "#212121",
		"list.focusForeground": "#fff",
		"list.inactiveSelectionBackground": "#212121",
		"list.inactiveFocusBackground": "#121212",
		"list.inactiveSelectionForeground": "#fff",
		"list.highlightForeground": "#fff",
		"statusBar.background": "#121212",
		"statusBar.foreground": "#b3b3b3",
		"statusBar.border": "#212121",
		"statusBar.debuggingBackground": "#212121",
		"statusBar.debuggingForeground": "#ffffff",
		"statusBarItem.activeBackground": "#212121",
		"statusBarItem.hoverBackground": "#212121",
		"statusBar.noFolderBackground": "#121212",
		"statusBar.noFolderBorder": "#121212",
		"statusBar.noFolderForeground": "#b3b3b3",
		"notificationCenter.border": "#121212",
		"notificationCenterHeader.foreground": "#b3b3b3",
		"notificationCenterHeader.background": "#121212",
		"editorGroup.border": "#9B59B6",
		"editorGroup.dropBackground": "#212121",
		"tab.activeBackground": "#212121"
	},
	"tokenColors": [
		{
			"name": "Comentários",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#6A9955",
				"fontStyle": ""
			}
		},
		{
			"name": "Storage Type (var, function, class) - Javascript",
			"scope": [
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#C35590",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Nome da variável - Javascript",
			"scope": [
				"variable"
			],
			"settings": {
				"foreground": "#CD97B4",
				"fontStyle": ""
			}
		},
		{
			"name": "Nome do import - Javascript",
			"scope": [
				"meta.import variable.other.readwrite.alias"
			],
			"settings": {
				"foreground": "#F8F8F2",
				"fontStyle": ""
			}
		},
		{
			"name": "Keywords (import, export, require) - Javascript",
			"scope": [
				"keyword"
			],
			"settings": {
				"foreground": "#C35590",
				"fontStyle": ""
			}
		},
		{
			"name": "Parênteses, vírgulas e coisas do tipo - Javascript",
			"scope": [
				"source"
			],
			"settings": {
				"foreground": "#F8F8F2",
				"fontStyle": ""
			}
		},
		{
			"name": "String - Javascript",
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#F1FA8C",
				"fontStyle": ""
			}
		},
		{
			"name": "Nome de Classe e Herança - Javascript",
			"scope": [
				"entity.name.class",
				"entity.name.type.class",
				"entity.other.inherited-class"
			],
			"settings": {
				"foreground": "#CD97B4",
				"fontStyle": ""
			}
		},
		{
			"name": "Nome de métodos - Javascript",
			"scope": [
				"entity.name.function"
			],
			"settings": {
				"foreground": "#62949A",
				"fontStyle": ""
			}
		},
		{
			"name": "Tag HTML",
			"scope": [
				"entity.name.tag"
			],
			"settings": {
				"foreground": "#B699C2",
				"fontStyle": ""
			}
		},
		{
			"name": "Nome do atributo HTML",
			"scope": [
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#58C355",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Nome de componentes - Javascript",
			"scope": [
				"support.class.component.js"
			],
			"settings": {
				"foreground": "#B699C2",
				"fontStyle": ""
			}
		},
		{
			"name": "Embbed Expression - Javascript",
			"scope": [
				"punctuation.section.embedded.begin.js",
				"punctuation.section.embedded.end.js",
				"punctuation.definition.block.js",
				"punctuation.separator.key-value.js"
			],
			"settings": {
				"foreground": "#F7E3FF",
				"fontStyle": ""
			}
		},
		{
			"name": "Support functions - Javascript",
			"scope": [
				"support.function.js",
				"support.function.console.js"
			],
			"settings": {
				"foreground": "#62949A",
				"fontStyle": ""
			}
		},
		{
			"name": "Object Member - Javascript",
			"scope": [
				"meta.object.member.js"
			],
			"settings": {
				"foreground": "#98CD97",
				"fontStyle": ""
			}
		},
		{
			"name": "Nome do atributo - CSS",
			"scope": [
				"support.type.property-name.css"
			],
			"settings": {
				"foreground": "#C35590",
				"fontStyle": ""
			}
		},
		{
			"name": "Nome das unidades - CSS",
			"scope": [
				"keyword.other.unit"
			],
			"settings": {
				"foreground": "#CD97B4",
				"fontStyle": ""
			}
		},
	]
}